
@{
    ViewBag.Title = "GroupedBarChart";
}

<h2><u>Grouped BarChart </u></h2>
<style>
    body {
        font-family: "Arial", sans-serif;
    }

    .axis {
        font-size: 13px;
    }

        .axis path,
        .axis line {
            fill: none;
            stroke-width: 1px;
            stroke: #000;
            shape-rendering: crispEdges;
        }

    .label {
        font-size: 13px;
    }
</style>


<script src="~/Scripts/d3/d3.js"></script>
<script src="~/Scripts/d3/d3.min.js"></script>
<script src="~/Scripts/d3/d3-selection-multi.v1.min.js"></script>


<div id="bar-chart">
</div>




<script src="~/Scripts/custom/GroupedBarChart.js"></script>
<script>
    $("#GroupedBarChart").addClass('active');
</script>



