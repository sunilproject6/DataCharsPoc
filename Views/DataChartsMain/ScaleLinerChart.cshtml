
@{
    ViewBag.Title = "ScaleLinerChart";
}

<h2><u>Scale Liner Chart</u></h2>
<style >
    .line {
    fill: none;
    stroke: #ffab00;
    stroke-width: 3;
}

.overlay {
    fill: none;
    pointer-events: all;
}

/* Style the dots by assigning a fill and stroke */
.dot {
    fill: #ffab00;
    stroke: #fff;
}

.focus circle {
    fill: none;
    stroke: steelblue;
}

</style>
<script src="~/Scripts/d3/d3.js"></script>
<script src="~/Scripts/d3/d3.min.js"></script>

<div id="scaleLinerdiv"></div>
<script src="~/Scripts/custom/ScaleLiner.js"></script>


<script>
    $("#scaleliner").addClass('active');
</script>


